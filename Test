public class Test {

	public static void main(String[] args) 
	{
	JFrame jiff=new JFrame("Calculator");
	JFrame jaff=new JFrame("Drawing");
	DrawingPanel dp=new DrawingPanel();
	jaff.getContentPane().add(dp);
	jaff.setSize(300, 200);
	jaff.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	jaff.setVisible(true);
	
	Calculator calc=new Calculator();
	jiff.getContentPane().add(calc);
	jiff.setSize(300, 200);
	jiff.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	jiff.setVisible(true);
	

	}

}
